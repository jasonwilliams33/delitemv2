        <mobile_callable>false</mobile_callable>
        <name>DebugUtils</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var DebugUtils = Class.create();
DebugUtils.prototype = {
    
	initialize: function() {
		this.DEBUG = gs.getProperty('cc.debug_utils.debug', 'false') == 'true';
        this.DEBUG_PREFIX = '>>>DEBUG: DebugUtils';
    },

	/**SNDOC
      @name debug
      @description Writes a given debug message to the log
	  @param {string} [timestamp] - The time in miliseconds when the method is called
      @param {string} [source] - The script/artifact from where the method is called
	  @param {string} [message] - The message to write to the log
	  @param {string} [debugMethod] - The type of log message (info, error, warn, debug, log)
	  @param {boolean} [debugOverride] - Allows you to write the message to the log even if logging is disabled
      @returns {boolean} Whether the message was written to the logs or not
    */
    debug: function(timestamp, source, message, debugMethod, debugOverride) {
        if (!this.DEBUG && !debugOverride) {
            return false;
        }

        var debugMethods = ['info', 'error', 'warn', 'debug'];
        debugMethod = (debugMethods.indexOf(debugMethod) != -1) ? debugMethod : 'info';

        gs[debugMethod](this.DEBUG_PREFIX + ' - Timestamp: ' + timestamp + ' - ' + source + '\n\n' + message);

        return true;
    },

    type: 'DebugUtils'
};]]></script>
